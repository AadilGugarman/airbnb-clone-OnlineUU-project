# Cloudinary Configuration
# (Get from Cloudinary Dashboard → API Keys section)
CLOUD_NAME=
CLOUD_API_KEY=
CLOUD_API_SECRET=

# Mapbox Access Token
# (Get from Mapbox Account → Tokens → Default public token)
MAP_TOKEN=
