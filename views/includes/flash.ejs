<!--Success flash -->
<% if(success && success.length){ %>
<div
  class="col-4 mx-auto alert alert-success alert-dismissible fade show"
  role="alert"
>
  <%=success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>


<!-- Error flash -->
<%}%> <% if(error && error.length){ %>
<div
  class="col-4 mx-auto alert alert-danger alert-dismissible fade show"
  role="alert"
>
  <%=error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
